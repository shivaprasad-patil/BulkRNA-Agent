# Environment variables for BulkRNA Agent

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
REASONING_MODEL=gpt-oss:20b
BIOMEDICAL_MODEL=cniongolo/biomistral

# Analysis Thresholds
FDR_THRESHOLD=0.05
LOG2FC_THRESHOLD=1.0
MIN_COUNT_THRESHOLD=10

# Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/bulkrna_agent.log

# Data Directories
UPLOAD_DIR=./data/uploads
OUTPUT_DIR=./data/outputs
CACHE_DIR=./data/cache

# MCP Server
USE_MCP_SERVER=true

# Web Interface
HOST=127.0.0.1
PORT=7860
SHARE=false
